


import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int a= sc.nextInt();
        int b= sc.nextInt();
        System.out.println("Enter first number: ");
        System.out.println("Enter second number: ");
        int firstdigit=firstdigit/10;
        int second_digit=(second_digit%10);
        int sum=firstdigit+second_digit;
        int product=firstdigit*second_digit;
        int Sum=sum+product;
            if(int Sum=a) 
            {
                System.out.printf(a);
            }
            if(int Sum=b) 
            {
                System.out.printf(b);
            }
            else{
                System.out.println("Not a special number");
            }
    }
}



